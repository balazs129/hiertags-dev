<VirtualHost *:80>
    ServerName hiertags-dev.elte.hu
    DocumentRoot /home/hiertags/hiertags-dev/
    ServerAdmin pollner@hal.elte.hu

    Alias /static/ /home/hiertags/hiertags-dev/static/
    <Directory /home/hiertags/static/>
        Order deny,allow
        Allow from all
    </Directory>

    WSGIDaemonProcess hiertags user=hiertags group=hiertags processes=2 threads=5
    WSGIProcessGroup hiertags

    WSGIScriptAlias / /home/hiertags/hiertags-dev/hiertags/wsgi.py
</VirtualHost>

